{
  "name": "mq-web3",
  "version": "1.0.0",
  "main": "dist/bundle.js",
  "repository": "https://github.com/HanTengfeifei/mq-web3.git",
  "author": "Tengfei Han",
  "files": [
    "dist",
    "readme.md"
  ],
  "keywords": [
    "chat",
    "messaging",
    "conversation"
  ],
  "scripts": {
    "dev": "set NODE_ENV=developemnt&& rollup -c rollup.config.js -w",
    "build": "rm -fr dist && set NODE_ENV=production&& rollup -c rollup.config.js",
    "clean": "rimraf 'dist/*'",
    "manifest": "mm-snap manifest --fix",
    "eval": "mm-snap eval -b dist/index.js",
    "serve": "concurrently \"rollup --config --watch\" \"mm-snap serve\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "types": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.12.0",
    "concurrently": "7.1.0",
    "eslint": "^8.9.0",
    "husky": "3.0.9",
    "lint-staged": "^11.2.6",
    "rimraf": "3.0.2",
    "rollup": "^2.67.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-execute": "1.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@noble/ed25519": "^1.6.1",
    "@protobuf-ts/plugin": "^2.7.0",
    "js-sha3": "^0.8.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
